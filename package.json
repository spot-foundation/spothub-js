{
  "name": "spotspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "APP_ENV=test node --async-stack-traces --harmony -r esm-wallaby --experimental-vm-modules node_modules/.bin/jest --forceExit"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "\\.m?jsx?$": "jest-esm-transformer"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "globalTeardown": "./jest/teardown.js",
    "globalSetup": "./jest/setup.js",
    "setupFilesAfterEnv": [
      "./jest/aroundTest.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spot-foundation/spotspace-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spot-foundation/spotspace-js/issues"
  },
  "homepage": "https://github.com/spot-foundation/spotspace-js#readme",
  "devDependencies": {
    "blakejs": "^1.1.1",
    "express": "^4.17.1",
    "express-paths-as-routes": "^1.0.0",
    "http": "^0.0.1-security",
    "jest": "^27.3.1",
    "jest-esm-transformer": "^1.0.0",
    "multer": "^1.4.3",
    "test-listen": "^1.1.0", 
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "esm-wallaby": "^3.2.25",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-form-data": "^2.0.0",
    "object-to-formdata": "^4.2.2"
  },
  "engines": {
    "node": "16",
    "npm": ">= 6"
  },
  "esm": {
    "cjs": {
      "dedefault": true,
      "topLevelReturn": true
    },
    "mainFields": [
      "module",
      "main"
    ],
    "mode": "auto"
  }
}
